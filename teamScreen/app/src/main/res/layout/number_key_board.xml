<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/key_board"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingLeft="@dimen/keyboard_margin_left"
    android:paddingRight="@dimen/keyboard_margin_right"
    android:paddingTop="@dimen/keyboard_margin_bottom_10">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/keyboard_margin_bottom"
            android:orientation="horizontal">

            <com.dariopellegrini.formbuilder.FineTextView
                android:id="@+id/key_board_7"
                android:layout_width="@dimen/dp_140"
                android:layout_height="@dimen/dp_84"
                android:background="@drawable/bg_key_board_selector"
                android:gravity="center"
                android:text="7"
                android:textColor="@color/button_text_key_board_green_selector"
                android:textSize="@dimen/keyboard_text_size" />

            <com.dariopellegrini.formbuilder.FineTextView
                android:id="@+id/key_board_8"
                android:layout_width="@dimen/dp_140"
                android:layout_height="@dimen/dp_84"
                android:layout_marginLeft="@dimen/keyboard_margin_left"
                android:background="@drawable/bg_key_board_selector"
                android:gravity="center"
                android:text="8"
                android:textColor="@color/button_text_key_board_green_selector"
                android:textSize="@dimen/keyboard_text_size" />


            <com.dariopellegrini.formbuilder.FineTextView
                android:id="@+id/key_board_9"
                android:layout_width="@dimen/dp_140"
                android:layout_height="@dimen/dp_84"
                android:layout_marginLeft="@dimen/keyboard_margin_left"
                android:background="@drawable/bg_key_board_selector"
                android:gravity="center"
                android:text="9"
                android:textColor="@color/button_text_key_board_green_selector"
                android:textSize="@dimen/keyboard_text_size" />

            <ImageView
                android:id="@+id/delete_icon"
                android:layout_width="@dimen/dp_170"
                android:layout_height="@dimen/dp_84"
                android:layout_marginLeft="@dimen/keyboard_margin_left"
                android:background="@drawable/bg_key_board_selector"
                android:gravity="center"
                android:padding="@dimen/dp_16"
                android:src="@drawable/bg_key_board_delete_selector"
                android:textSize="@dimen/keyboard_text_size" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/keyboard_margin_bottom"
            android:orientation="horizontal">

            <com.dariopellegrini.formbuilder.FineTextView
                android:id="@+id/key_board_4"
                android:layout_width="@dimen/dp_140"
                android:layout_height="@dimen/dp_84"
                android:background="@drawable/bg_key_board_selector"
                android:gravity="center"
                android:text="4"
                android:textColor="@color/button_text_key_board_green_selector"
                android:textSize="@dimen/keyboard_text_size" />

            <com.dariopellegrini.formbuilder.FineTextView
                android:id="@+id/key_board_5"
                android:layout_width="@dimen/dp_140"
                android:layout_height="@dimen/dp_84"
                android:layout_marginLeft="@dimen/keyboard_margin_left"
                android:background="@drawable/bg_key_board_selector"
                android:gravity="center"
                android:text="5"
                android:textColor="@color/button_text_key_board_green_selector"
                android:textSize="@dimen/keyboard_text_size" />

            <com.dariopellegrini.formbuilder.FineTextView
                android:id="@+id/key_board_6"
                android:layout_width="@dimen/dp_140"
                android:layout_height="@dimen/dp_84"
                android:layout_marginLeft="@dimen/keyboard_margin_left"
                android:background="@drawable/bg_key_board_selector"
                android:gravity="center"
                android:text="6"
                android:textColor="@color/button_text_key_board_green_selector"
                android:textSize="@dimen/keyboard_text_size" />


            <com.dariopellegrini.formbuilder.FineTextView
                android:id="@+id/tx_empty"
                android:layout_width="@dimen/dp_170"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/keyboard_margin_left"
                android:background="@drawable/bg_key_board_selector"
                android:gravity="center"
                android:paddingBottom="@dimen/dp_10"
                android:paddingLeft="@dimen/dp_52"
                android:paddingRight="@dimen/dp_52"
                android:paddingTop="@dimen/dp_10"
                android:text="@string/empty"
                android:textColor="@color/button_text_key_board_green_selector"
                android:textSize="@dimen/keyboard_text_size_1" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/keyboard_margin_bottom"
                    android:orientation="horizontal">

                    <com.dariopellegrini.formbuilder.FineTextView
                        android:id="@+id/key_board_1"
                        android:layout_width="@dimen/dp_140"
                        android:layout_height="@dimen/dp_84"
                        android:background="@drawable/bg_key_board_selector"
                        android:gravity="center"
                        android:text="1"
                        android:textColor="@color/button_text_key_board_green_selector"
                        android:textSize="@dimen/keyboard_text_size" />

                    <com.dariopellegrini.formbuilder.FineTextView
                        android:id="@+id/key_board_2"
                        android:layout_width="@dimen/dp_140"
                        android:layout_height="@dimen/dp_84"
                        android:layout_marginLeft="@dimen/keyboard_margin_left"
                        android:background="@drawable/bg_key_board_selector"
                        android:gravity="center"
                        android:text="2"
                        android:textColor="@color/button_text_key_board_green_selector"
                        android:textSize="@dimen/keyboard_text_size" />

                    <com.dariopellegrini.formbuilder.FineTextView
                        android:id="@+id/key_board_3"
                        android:layout_width="@dimen/dp_140"
                        android:layout_height="@dimen/dp_84"
                        android:layout_marginLeft="@dimen/keyboard_margin_left"
                        android:background="@drawable/bg_key_board_selector"
                        android:gravity="center"
                        android:text="3"
                        android:textColor="@color/button_text_key_board_green_selector"
                        android:textSize="@dimen/keyboard_text_size" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <com.dariopellegrini.formbuilder.FineTextView
                        android:layout_width="@dimen/dp_140"
                        android:layout_height="@dimen/dp_84"
                        android:background="@drawable/bg_key_board_default"
                        android:gravity="center"
                        android:text=""
                        android:textSize="@dimen/keyboard_text_size" />

                    <com.dariopellegrini.formbuilder.FineTextView
                        android:id="@+id/key_board_0"
                        android:layout_width="@dimen/dp_140"
                        android:layout_height="@dimen/dp_84"
                        android:layout_marginLeft="@dimen/keyboard_margin_left"
                        android:background="@drawable/bg_key_board_selector"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/button_text_key_board_green_selector"
                        android:textSize="@dimen/keyboard_text_size" />

                    <com.dariopellegrini.formbuilder.FineTextView
                        android:layout_width="@dimen/dp_140"
                        android:layout_height="@dimen/dp_84"
                        android:layout_marginLeft="@dimen/keyboard_margin_left"
                        android:background="@drawable/bg_key_board_default"
                        android:gravity="center"
                        android:text=""
                        android:textSize="@dimen/keyboard_text_size" />


                </LinearLayout>

            </LinearLayout>

            <com.dariopellegrini.formbuilder.FineTextView
                android:id="@+id/tx_confirm"
                android:layout_width="@dimen/dp_170"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/keyboard_margin_left"
                android:background="@drawable/bg_key_board_green_selector"
                android:gravity="center"
                android:paddingBottom="@dimen/dp_10"
                android:paddingLeft="@dimen/dp_52"
                android:paddingRight="@dimen/dp_52"
                android:paddingTop="@dimen/dp_10"
                android:text="@string/confirm"
                android:textColor="@color/white"
                android:textSize="@dimen/keyboard_text_size_1" />
        </LinearLayout>

    </LinearLayout>


</LinearLayout>